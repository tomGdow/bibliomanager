<!doctype html>
<html lang="en" ng-app="myApp">
<head>
    <meta charset="UTF-8">
    <title>Bibliography Manager</title>

    <%= stylesheet_link_tag "application", :media => "all" %>
    <link rel="stylesheet" type="text/css" href="//code.jquery.com/ui/1.11.1/themes/smoothness/jquery-ui.css">

    <%= javascript_include_tag "vendor/modernizr.js" %>
    <script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
    <script src=" https://ajax.googleapis.com/ajax/libs/angularjs/1.2.21/angular.min.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.3.0-beta.15/angular-animate.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.0.7/angular-resource.min.js"></script>



    <%= csrf_meta_tags %>
</head>
<body ng-controller="MyCtrlIndex">
     <h1>Hello from new index</h1>

     <div id = "ngView"  class="small-12" ng-view></div> <!--ng-view-->

     <%= javascript_include_tag "application" %>
